{
  "name": "example-mcp-server",
  "version": "0.0.1",
  "description": "Example MCP server using the TypeScript MCP SDK",
  "main": "src/server.ts",
  "engines": {
    "bun": ">=1.0.0"
  },
  "scripts": {
    "dev": "bun --watch src/server.ts",
    "build": "bun build src/server.ts --outfile dist/server.js",
    "start": "bun src/server.ts",
    "clean": "rm -rf dist"
  },
  "keywords": [],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/bun": "^1.1.0",
    "@types/cors": "^2.8.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.0",
    "@types/node": "^20.19.9",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  }
}
